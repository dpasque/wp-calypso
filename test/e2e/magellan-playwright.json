{
	"mocha_tests": [ "specs-playwright" ],
	"mocha_args": "--file lib/mocha-hooks/playwright-hooks.ts --reporter mocha-teamcity-reporter -R spec-junit-reporter",
	"max_test_attempts": 3,
	"max_workers": 1,
	"sauce": false,
	"framework": "testarmada-magellan-mocha-plugin",
	"reporters": [ "./lib/reporter/magellan-reporter.js" ],
	"executors": [ "testarmada-magellan-local-executor" ],
	"bail_time": 300000
}
